{
  "name": "@example/minimal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "netlify dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "run-s format:*",
    "format:prettier": "prettier -w --plugin-search-dir=. .",
    "format:eslint": "eslint --fix \"**/*.{js,ts,jsx,tsx,mjs,cjs,astro}\"",
    "lint": "run-s --print-label --continue-on-error lint:*",
    "lint:prettier": "prettier --check --plugin-search-dir=. .",
    "lint:eslint": "eslint \"**/*.{js,ts,jsx,tsx,mjs,cjs,astro}\"",
    "lint:tsc": "tsc --noEmit",
    "lint:astro": "cd src && astro check"
  },
  "dependencies": {
    "@astrojs/netlify": "^2.2.0",
    "title": "^3.5.3"
  },
  "devDependencies": {
    "@astrojs/site-kit": "withastro/site-kit#v1.0.0",
    "@astrojs/tailwind": "^3.1.1",
    "@netlify/functions": "^1.4.0",
    "@types/node": "^18.15.5",
    "@types/title": "^3.4.1",
    "astro": "^2.1.5",
    "astro-icon": "^0.8.0",
    "eslint": "^8.36.0",
    "install": "^0.13.0",
    "netlify-cli": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.6",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2"
  },
  "license": "MIT"
}
